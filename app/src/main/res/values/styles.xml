<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CalenderViewCustom" parent="Theme.AppCompat">
        <item name="colorAccent">#FFC107</item>
        <item name="colorPrimary">#7E7E7E</item>
        <item name="android:textColorPrimary">#7E7E7E</item>
    </style>

    <style name="CalenderViewDateCustomText" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">#7E7E7E</item>
        <item name="android:weekNumberColor">#FFC107</item>
    </style>

    <style name="CalenderViewWeekCustomText" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">#7E7E7E</item>
    </style>

    <style name="SCBSwitch" parent="Theme.AppCompat.Light">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">#FFC107</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">#f1f1f1
        </item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">#42221f1f
        </item>
    </style>
</resources>